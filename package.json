{
  "name": "saucedemo-playwright",
  "version": "1.0.0",
  "description": "Automated software testing with playwright",
  "main": "index.js",
  "scripts": {
    "test:chrome": "playwright test --config=playwright.config.ts --project=Chromium --reporter=html",
    "test:firefox": "playwright test --config=playwright.config.ts --project=Firefox",
    "test:webkit": "playwright test --config=playwright.config.ts --project=Webkit"
  },
  "author": "JDD",
  "license": "ISC",
  "dependencies": {
    "@playwright/test": "^1.49.1",
    "prettier": "^3.4.2"
  },
  "directories": {
    "test": "tests"
  }
}
